<h1>Create Song</h1>
<%= form_for @song do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :artist_name %>
  <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>

<% if @song.released %>
  <h3>Released <%= @song.released_year %></h3>
<% else %>
  <h3>Not Officially released
    <% if @song.released_year %>
       - <%= @song.released_year %>
    <% end %>
  </h3>
<% end %>
